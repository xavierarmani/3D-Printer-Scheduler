<%= form_for(@schedule) do |f| %>
  <% if @schedule.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@schedule.errors.count, "error") %> prohibited this admin from being saved:</h2>

      <ul>
      <% @schedule.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <p>Enter your username in the reserved field to make a reservation.</p>
  <div class="field">
    <%= f.label :machineID %><br>
    <%= f.select :machineID, ['Machine 1', 'Machine 2', 'Machine 3', 'Machine 4'] %>
  </div>
  
  <div class="field">
    <%= f.label :userName %><br>
    <%= f.text_field :userName %>
  </div>
  
  <div class="field">
    <%= f.label :startTime %><br>
    <%= f.text_field :startTime %>
  </div>
  <div class="field">
    <%= f.label :endTime %><br>
    <%= f.text_field :endTime %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
